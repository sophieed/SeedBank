<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="179.0" prefWidth="431.0" spacing="5.0"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.controllers.FinishedController">
	<children>
		<HBox prefHeight="87.0" prefWidth="421.0">
			<children>
				<Label prefWidth="115.0" text="Performance Notes:">
					<HBox.margin>
						<Insets left="10.0" right="10.0" />
					</HBox.margin>
				</Label>
				<TextArea fx:id="performanceNotes" prefHeight="72.0"
					prefWidth="280.0">
					<HBox.margin>
						<Insets right="10.0" />
					</HBox.margin>
				</TextArea>
			</children>
		</HBox>
		<VBox prefHeight="35.0" prefWidth="421.0">
			<children>
				<Label text="Star Rating:">
					<VBox.margin>
						<Insets left="10.0" top="10.0" />
					</VBox.margin>
				</Label>
			</children>
		</VBox>
		<HBox prefHeight="29.0" prefWidth="421.0" spacing="5.0">
			<children>
				<Button fx:id="saveFinishedButton" mnemonicParsing="false"
					onAction="#save" text="Save" />
				<Button fx:id="cancelButton" cancelButton="true"
					mnemonicParsing="false" onAction="#cancel" text="Cancel" />
			</children>
			<padding>
				<Insets bottom="10.0" left="10.0" top="10.0" />
			</padding>
		</HBox>
	</children>
	<padding>
		<Insets left="10.0" top="10.0" />
	</padding>
</VBox>
