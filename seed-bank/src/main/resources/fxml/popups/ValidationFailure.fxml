<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="BOTTOM_RIGHT" maxHeight="-Infinity"
	maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
    prefWidth="450.0"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.controllers.ValidationFailureController">
	<children>
		<Label fx:id="causeOfFailure" contentDisplay="CENTER" alignment="CENTER" textAlignment="CENTER"
			prefWidth="450.0" wrapText="true">
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
		</Label>
		<Button fx:id="okButton" alignment="BOTTOM_RIGHT"
			mnemonicParsing="false" text="   Ok   " onAction="#ok">
			<VBox.margin>
				<Insets bottom="10.0" right="10.0" top="10.0" />
			</VBox.margin>
		</Button>
	</children>
</VBox>
